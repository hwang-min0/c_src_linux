#include <stdio.h>

int main(void)
{
   int man, woman;
   scanf("%d %d", $man, &woman);
   
   double 
   
   double manRatio = 1.0 * man / (man + woman) * 100;
   double womanRatio = 1.0 * woman / (man + woman) * 100;
   
   printf("manRatio: %.2f%%\twomanRatio: %.2f%%\n", manRatio, womanRatio);
   return 0;

}
